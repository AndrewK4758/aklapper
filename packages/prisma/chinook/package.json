{
  "name": "@aklapper/chinook-client",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@aklapper/types": "workspace:*"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "module": "./src/index.js",
  "nx": {
    "sourceRoot": "packages/prisma/chinook/src",
    "root": "packages/prisma/chinook",
    "projectType": "library",
    "name": "chinook-client",
    "targets": {
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/prisma/chinook",
          "command": "pnpx prisma generate"
        }
      },
      "pull-db": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/prisma/chinook",
          "command": "pnpx prisma db pull"
        }
      }
    },
    "push-db": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/prisma/chinook",
        "command": "pnpx prisma db push"
      }
    }
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
