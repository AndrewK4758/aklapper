{
  "name": "@aklapper/games-client",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "module": "./dist/src/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "client": "./dist/generated/index.js",
      "client-types": "./dist/generated/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "nx": {
    "sourceRoot": "packages/prisma/games/src",
    "root": "packages/prisma/games",
    "projectType": "library",
    "name": "games-client",
    "targets": {
      "generate": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/prisma/games",
          "command": "pnpx prisma generate && node scripts/fix-prisma-types.cjs"
        }
      },
      "pull-db": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/prisma/games",
          "command": "pnpx prisma db pull"
        }
      },
      "build": {
        "dependsOn": [
          "generate"
        ]
      }
    }
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
