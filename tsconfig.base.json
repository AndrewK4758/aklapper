{
  "compilerOptions": {
    "allowJs": false,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "emitDecoratorMetadata": false,
    "esModuleInterop": true,
    "experimentalDecorators": false,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "incremental": true,
    "isolatedModules": true,
    "lib": ["ESNext", "DOM", "DOM.AsyncIterable"],
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "pretty": true,
    "removeComments": false,
    "resolveJsonModule": false,
    "rootDir": ".",
    "skipDefaultLibCheck": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "target": "ESNext",
    "verbatimModuleSyntax": false,
    "paths": {
      "@aklapper/all-games-map": ["libs/models/all-games-map/src/index.ts"],
      "@aklapper/chain": ["libs/chain/src/index.ts"],
      "@aklapper/chutes-and-ladders": ["libs/games/chutes-and-ladders/src/index.ts"],
      "@aklapper/game": ["libs/game/src/index.ts"],
      "@aklapper/game-builder": ["libs/models/game-builder/src/index.ts"],
      "@aklapper/game-instance": ["libs/models/game-instance/src/index.ts"],
      "@aklapper/games-chains": ["libs/games-chains/src/index.ts"],
      "@aklapper/games-components": ["libs/games-components/src/index.ts"],
      "@aklapper/media-recorder": ["libs/media-recorder/src/index.ts"],
      "@aklapper/mocks": ["libs/mocks/src/index.ts"],
      "@aklapper/password": ["libs/password/src/index.ts"],
      "@aklapper/prisma": ["libs/prisma/src/index.ts"],
      "@aklapper/prompt-builder": ["libs/gen-ai/prompt-builder/src/index.ts"],
      "@aklapper/react-shared": ["libs/react-shared/src/index.ts"],
      "@aklapper/rule": ["libs/models/rule/src/index.ts"],
      "@aklapper/socket-io-client": ["libs/socket-io/client/src/index.ts"],
      "@aklapper/socket-io-server": ["libs/socket-io/server/src/index.ts"],
      "@aklapper/tic-tac-toe": ["libs/games/tic-tac-toe/src/index.ts"],
      "@aklapper/types-ai": ["libs/types/types-ai/src/index.ts"],
      "@aklapper/types-api": ["libs/types/types-api/src/index.ts"],
      "@aklapper/types-game": ["libs/types/types-game/src/index.ts"],
      "@aklapper/utils": ["libs/utils/src/index.ts"],
      "@aklapper/vertex-ai": ["libs/gen-ai/vertex-ai/src/index.ts"]
    }
  }
}
