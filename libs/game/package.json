{
  "name": "@aklapper/game",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "import": "./index.js"
    }
  },
  "dependencies": {},
  "nx": {
    "name": "game",
    "sourceRoot": "libs/game/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "cwd": "libs/game",
          "color": true,
          "command": "vite build"
        }
      }
    }
  }
}