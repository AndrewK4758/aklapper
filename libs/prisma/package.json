{
  "name": "@aklapper/prisma",
  "private": true,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.d.ts",
      "require": null,
      "default": "./index.js"
    }
  },
  "main": "./index.js",
  "types": "./src/index.d.ts",
  "type": "module",
  "dependencies": {},
  "nx": {
    "name": "prisma",
    "sourceRoot": "libs/prisma/src",
    "projectType": "library",
    "targets": {
      "generate": {
        "command": "pnpx prisma generate",
        "options": {
          "cwd": "./libs/prisma"
        }
      },
      "build": {
        "executor": "@nx/js:tsc",
        "options": {
          "main": "libs/prisma/src/index.ts",
          "command": "tsc --build tsconfig.lib.json --pretty --verbose ",
          "outputPath": "dist/libs/prisma",
          "tsConfig": "libs/prisma/tsconfig.lib.json"
        }
      }
    }
  }
}