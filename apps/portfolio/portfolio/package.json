{
  "name": "@aklapper/portfolio",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./index-[hash].js",
      "require": null,
      "default": "./index-[hash].js"
    }
  },
  "main": "./index-[hash].js",
  "dependencies": {},
  "nx": {
    "name": "portfolio",
    "sourceRoot": "apps/portfolio/portfolio/src",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "^typecheck"
        ],
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "cwd": "apps/portfolio/portfolio",
          "color": true,
          "command": "vite build"
        }
      },
      "deploy": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "defaultConfiguration": "production",
        "options": {
          "commands": [
            {
              "command": "gcloud app deploy website.yaml --quiet --verbosity=debug"
            }
          ],
          "env": {
            "NODE_ENV": "production"
          }
        }
      }
    }
  }
}
