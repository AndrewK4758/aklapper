{
  "name": "portfolio",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/portfolio/portfolio/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": [
          {
            "command": "gcloud app deploy website.yaml --quiet --verbosity=debug"
          }
        ],
        "env": {
          "NODE_ENV": "production"
        }
      }
    }
  }
}
