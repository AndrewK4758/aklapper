{
  "name": "local-model",
  "type": "module",
  "private": true,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./index-[hash].js",
      "require": null,
      "default": "./index-[hash].js"
    }
  },
  "main": "./index-[hash].js",
  "nx": {
    "name": "local-model",
    "projectType": "application",
    "sourceRoot": "apps/local-model/local-model/src",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "cwd": "apps/local-model/local-model",
          "color": true,
          "command": "vite build"
        }
      }
    }
  }
}